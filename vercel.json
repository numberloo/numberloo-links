{
    "routes": [
        {
            "src": "/question/(.*)",
            "dest": "/question/index.html"
        },
        {
            "src": "/.well-known/(.*)",
            "dest": "/.well-known/$1"
        },
        {
            "src": "/apple-app-site-association",
            "dest": "/apple-app-site-association"
        }
    ],
    "rewrites": [
        {
            "source": "/question/:path*",
            "destination": "/question/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}
