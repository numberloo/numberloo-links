{
    "rewrites": [
        {
            "source": "/question/:path*",
            "destination": "/question/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "trailingSlash": false,
    "outputDirectory": "public"
}
